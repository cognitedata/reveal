{
  "name": "@cognite/node-visualizer",
  "version": "1.2.15",
  "description": "Spatial visualizer for Cognite",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --env.debug=false --config webpack.config.js",
    "build:profile": "webpack --env.debug=false --env.profile=true --config webpack.config.js",
    "build:analyze": "yarn build:profile && webpack-bundle-analyzer ./dist/stats.json",
    "build:watch": "webpack --env.debug=false --config webpack.config.js --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:dev": "webpack --env.debug=true --config webpack.config.js",
    "watch:css": "sass --watch src/UserInterface/styles/scss:src/UserInterface/styles/css",
    "start": "node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode development --open --hot --config ./src/App/webpack.config.serve.js",
    "clean": "tsc --build --clean",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "predeploy": "cd standalone && npm install && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitedata/node-visualizer.git"
  },
  "keywords": [
    "subsurface",
    "cognite",
    "spatial-visualization",
    "3d",
    "react",
    "node-visualizer",
    "subsurface-visualization"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cognitedata/node-visualizer/issues"
  },
  "homepage": "https://github.com/cognitedata/node-visualizer#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@cognite/cogs.js": "^2.5.0",
    "@cognite/eslint-config": "^1.6.1",
    "@cognite/eslint-plugin": "^1.3.8",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.2",
    "@types/lodash": "^4.14.157",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^14.0.14",
    "@types/react": "^17.0.5",
    "@types/react-color": "^3.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-redux": "^7.1.16",
    "@types/react-virtualized": "^9.21.10",
    "@types/styled-components": "^5.1.0",
    "@types/ts-nameof": "^4.2.1",
    "@types/utm": "^1.1.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.6.0",
    "dotenv-webpack": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.1",
    "ts-loader": "^7.0.5",
    "typescript": "<4.1.0",
    "url-loader": "^4.1.0",
    "webpack": "4.42.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "@cognite/cogs.js": "^2.5.0",
    "@reduxjs/toolkit": "^1.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "^7.2.4",
    "redux": "4.0.5",
    "styled-components": "^5.1.1"
  },
  "dependencies": {
    "@cognite/geospatial-sdk-js": "^0.0.6",
    "@cognite/potree-core": "1.0.6",
    "@cognite/seismic-sdk-js": "0.2.1",
    "@reduxjs/toolkit": "^1.5.1",
    "camera-controls": "^1.22.1",
    "color": "^3.1.2",
    "immer": "^7.0.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "react-draggable": "^4.4.3",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-split-pane": "^0.1.91",
    "react-virtualized": "^9.21.2",
    "redux": "4.0.5",
    "styled-components": "^5.2.0",
    "three": "^0.119.0",
    "utm": "^1.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "browserslist": [
    "defaults"
  ]
}
