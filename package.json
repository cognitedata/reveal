    "@cognite/metrics": "^1.2.1",
    "@cognite/react-feature-flags": "^1.0.0",
    "@cognite/sdk": "^6.3.1",
    "@cognite/sdk-provider": "^2.0.0",
    "@visx/axis": "^3.0.0",
    "@visx/grid": "^3.0.0",
    "@visx/group": "^3.0.0",
    "@visx/responsive": "^3.0.0",
