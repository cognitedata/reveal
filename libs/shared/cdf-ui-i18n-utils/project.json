{
  "name": "cdf-ui-i18n-utils",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/cdf-ui-i18n-utils/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/cdf-ui-i18n-utils/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "format": ["esm", "cjs"],
        "outputPath": "dist/libs/shared/cdf-ui-i18n-utils",
        "tsConfig": "libs/shared/cdf-ui-i18n-utils/tsconfig.lib.json",
        "project": "libs/shared/cdf-ui-i18n-utils/package.json",
        "entryFile": "libs/shared/cdf-ui-i18n-utils/src/index.ts",
        "external": ["react", "react/jsx-runtime", "@cognite/cdf-utilities"],
        "rollupConfig": "libs/shared/cdf-ui-i18n-utils/rollup-config.js",
        "compiler": "tsc",
        "assets": [
          {
            "input": "libs/shared/cdf-ui-i18n-utils/bin",
            "glob": "*",
            "output": "./bin"
          },
          {
            "glob": "libs/shared/cdf-ui-i18n-utils/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/cdf-ui-i18n-utils/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
