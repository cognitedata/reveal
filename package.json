{
  "name": "@cognite/cdf-document-search-ui",
  "version": "0.2.1",
  "private": true,
  "contributors": [
    {
      "name": "Unstructured search (grep)",
      "url": "https://github.com/orgs/cognitedata/teams/grep"
    }
  ],
  "scripts": {
    "start": "env GENERATE_SOURCEMAP=false BROWSER=none WDS_SOCKET_PORT=3016 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3016 rescripts start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-document-search-ui -o index.js -d ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test",
    "lint": "eslint --config .eslintrc.production.js .",
    "lint:fix": "yarn lint --fix",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@cognite/cdf-utilities": "^0.14.4",
    "@cognite/cogs.js": "5.19.5",
    "@cognite/metrics": "1.2.2",
    "@cognite/react-feature-flags": "^1.0.0",
    "@cognite/sdk": "6.3.5",
    "@cognite/sdk-playground": "^5.10.2",
    "@cognite/sdk-provider": "^2.0.0",
    "@cognite/sdk-react-query-hooks": "6.0.3",
    "@cognite/z-index": "0.2.3",
    "@rescripts/cli": "0.0.16",
    "@rescripts/utilities": "0.0.8",
    "@sentry/browser": "5.30.0",
    "@xstate/inspect": "0.5.2",
    "@xstate/react": "1.6.3",
    "antd": "3.26.20",
    "framer-motion": "4.1.17",
    "fs-extra": "9.1.0",
    "i18next": "^21.6.14",
    "lodash": "4.17.21",
    "query-string": "6.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.16.2",
    "react-query": "3.34.16",
    "react-router": "6.2.2",
    "react-router-dom": "6.2.2",
    "react-scripts": "^5.0.0",
    "single-spa-react": "^4.0.0",
    "styled-components": "^5.2.3",
    "systemjs-webpack-interop": "2.3.7",
    "xstate": "4.30.3"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "1.10.7",
    "@cognite/cdf-ui-cli": "1.1.1",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "1.0.1",
    "@rescripts/rescript-use-babel-config": "0.0.12",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "26.0.24",
    "@types/lodash": "4.14.189",
    "@types/node": "14.18.33",
    "@types/react": "17.0.52",
    "@types/react-dom": "17.0.18",
    "@types/react-select": "3.1.2",
    "@types/react-table": "7.7.12",
    "@types/single-spa-react": "2.12.0",
    "@types/sinon": "9.0.11",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "4.3.8",
    "msw": "0.49.0",
    "postcss-prefixwrap": "1.37.0",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "randexp": "0.5.3",
    "sinon": "9.2.4",
    "style-loader": "1.3.0",
    "typescript": "4.6.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "14"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
