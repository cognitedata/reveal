{
  "name": "@cognite/data-exploration",
  "version": "3.3.2",
  "scripts": {
    "build": "yarn clean ; NODE_ENV=production rollup -c",
    "watch": "nodemon -e ts,tsx --ignore dist/ --exec \"env NODE_ENV=development rollup -c && yalc push --sig\"",
    "build:storybook": "build-storybook -s public",
    "clean": "rm -rf dist/",
    "lint": "eslint src --max-warnings 0 --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -p 9009 -s public",
    "test-storybook": "test-storybook --url http://localhost:9009",
    "chromatic": "npx chromatic",
    "semantic-release": "semantic-release"
  }
}
