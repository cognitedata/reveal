{
  "name": "@cognite/react-demo-app",
  "version": "0.4.0",
  "private": true,
  "scripts": {
    "analyse": "source-map-explorer 'build/static/js/*.js' --only-mapped --html /tmp/result.html && google-chrome /tmp/result.html",
    "start": "./scripts/start.sh",
    "build": "./scripts/build.sh",
    "test": "./scripts/test.sh",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "lint-fast": "pretty-quick --staged",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}' 'public/index.html'",
    "eslint:changed": "eslint --max-warnings 0 --color --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "testcafe:run": "./scripts/testcafe-run.sh",
    "testcafe:run-live": "./scripts/testcafe-run-live.sh",
    "testcafe:start": "./scripts/testcafe-serve-run-docker.sh",
    "testcafe:start-live": "./scripts/testcafe-start.sh",
    "testcafe:build": "./scripts/testcafe-build.sh",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cognite/cogs.js": "^3.9.0",
    "@cognite/metrics": "^1.1.0",
    "@cognite/react-container": "^1.2.1",
    "@cognite/react-errors": "^0.2.0",
    "@cognite/react-i18n": "^0.4.0",
    "@cognite/sdk": "^4.0.4",
    "@sentry/browser": "^6.2.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.10",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "styled-components": "^5.3.0"
  },
  "resolutions": {
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@cognite/eslint-config": "^2.0.1",
    "@cognite/eslint-plugin": "^2.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/testcafe": "^4.3.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.7",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.6",
    "@types/sinon": "^9.0.11",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "8.1.0",
    "concurrently": "^6.0.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^3.10.2",
