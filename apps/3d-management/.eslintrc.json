{
  "extends": ["plugin:@nrwl/nx/react", "../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "@typescript-eslint/no-loss-of-precision": ["warn"],
        "@typescript-eslint/ban-ts-comment": ["warn"]
      }
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-console": "warn"
      }
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
