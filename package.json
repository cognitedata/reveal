{
  "name": "@cognite/cdf-data-sets",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=3080 WDS_SOCKET_HOST=localhost PORT=3080 HTTPS=TRUE rescripts --max_old_space_size=2000 start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-data-sets -o index.js -d ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "prettier:fix": "prettier --write .prettierrc -l \"src/**/*.{js,ts,tsx}\"",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@cognite/cdf-utilities": "^0.12.0",
    "@cognite/cogs.js": "5.17.0",
    "@cognite/metrics": "^1.2.1",
    "@cognite/react-feature-flags": "^0.2.2-rc.0",
    "@cognite/sdk": "^6.3.1",
    "@cognite/sdk-provider": "^1.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.2",
    "@google-cloud/storage": "^5.15.3",
    "@rescripts/cli": "^0.0.16",
    "@rescripts/utilities": "^0.0.8",
    "@sentry/browser": "^6.13.3",
    "@types/history": "^4.7.9",
    "@types/lodash": "^4.14.175",
    "@types/react-router-dom": "^5.3.1",
    "antd": "^4.16.13",
    "commander": "^8.2.0",
    "cronstrue": "^1.119.0",
    "eslint-plugin-promise": "^5.1.1",
    "fs-extra": "^10.0.0",
    "i18next": "21.6.12",
    "query-string": "^7.0.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-i18next": "11.15.5",
    "react-iframe": "^1.8.0",
    "react-query": "^3.27.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.1",
    "systemjs-webpack-interop": "^2.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.1.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/rescript-use-babel-config": "^0.0.12",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/single-spa-react": "^2.12.0",
    "@types/sinon": "^10.0.4",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "concurrently": "^6.3.0",
    "cross-var": "^1.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "^7.0.2",
    "postcss-prefixwrap": "1.29.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "single-spa-react": "^3.2.0",
    "sinon": "^11.1.2",
    "style-loader": "^1.3.0",
    "typescript": "4.5.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "engines": {
    "node": "14"
  }
}
