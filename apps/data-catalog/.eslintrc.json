{
  "extends": [
    "plugin:cypress/recommended",
    "plugin:@nx/react",
    "../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "!**/*",
    "src/__mocks__/**/*"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "import/order": [
          "off"
        ],
        "@typescript-eslint/no-explicit-any": "off"
      }
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.cy.{ts,js,tsx,jsx}",
        "cypress/**/*.{ts,js,tsx,jsx}"
      ],
      "rules": {}
    }
  ]
}
