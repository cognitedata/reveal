{
  "name": "@cognite/applications",
  "private": true,
  "scripts": {
    "build": "bazel build //...",
    "test": "bazel test //...",
    "lint": "bazel test :eslint_test :prettier_test",
    "eslint": "bazel test :eslint_test",
    "eslint:fix": "bazel run :eslint_test -- --fix",
    "prettier": "bazel test :prettier_test",
    "prettier:fix": "bazel run :prettier_test -- --write",
    "postinstall": "patch-package",
    "lint-fast": "pretty-quick --staged",
    "prepare": "husky install",
    "generate": "plop --plopfile generators/index.ts",
    "update-helpers": "for i in `bazel query 'kind(generate_package_json_helpers, //...)'`; do bazel run $i; done"
  },
  "dependencies": {
    "@cognite/cogs.js": "^3.9.2",
    "@cognite/intercom-helper": "^0.1.0",
    "@cognite/metrics": "^1.1.0",
    "@cognite/react-container": "^1.2.5",
    "@cognite/react-errors": "^0.3.0",
    "@cognite/react-i18n": "^0.4.0",
    "@cognite/react-tenant-selector": "^1.2.7",
    "@cognite/sdk": "^4.0.4",
    "@sentry/browser": "^6.2.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.10",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.5",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@bazel/ibazel": "^0.15.10",
    "@bazel/typescript": "^3.5.0",
    "@cognite/eslint-config": "^2.0.1",
    "@cognite/eslint-plugin": "^2.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/testcafe": "^4.3.1",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.7",
    "@types/react-dom": "^17.0.3",
    "@types/react": "^17.0.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router-dom": "^5.1.6",
    "@types/sinon": "^9.0.11",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "8.1.0",
    "concurrently": "^6.0.2",
    "depcheck": "^1.4.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^6.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^12.0.0",
    "kill-port": "^1.6.1",
    "patch-package": "^6.2.2",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^11.3.2",
    "sinon": "^9.2.4",
    "testcafe": "^1.13.0",
    "ts-jest": "^26.5.6",
    "ts-node": "9",
    "typescript": "^4.1.5",
    "webpack": "4.44.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
