{
  "hosting": {
    "site": "cdf-ui-host-staging",
    "public": ".",
    "trailingSlash": false,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/",
        "destination": "/cdf",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "/cdf/apps/**",
        "function": "cdfProxy"
      },
      {
        "source": "/cdf/**",
        "destination": "/cdf/index.html"
      }
    ],
    "headers": [
      {
        "source": "/cdf/apps-manifest.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, no-cache, no-store, must-revalidate"
          },
          {
            "key": "x-app",
            "value": "cdf-host"
          }
        ]
      },
      {
        "source": "/cdf/import-map.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "/cdf/sub-apps-import-map.json",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=0, no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}
