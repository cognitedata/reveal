    "query-string": "^6.14.1",
    "react": "^18.2.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-highlight-words": "^0.18.0",
    "react-i18next": "^11.18.1",
    "react-query": "^3.16.1",
    "react-router-dom": "^6.4.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "recoil": "^0.7.5",
    "recoil-gear": "^1.0.2",
    "recoil-sync": "^0.1.1",
    "single-spa": "^5.8.3",
