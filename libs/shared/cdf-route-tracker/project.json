{
  "name": "cdf-route-tracker",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/cdf-route-tracker/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "npm": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": ["./scripts/npm.sh"]
      },
      "outputs": ["{workspaceRoot}/dist/libs/shared/cdf-route-tracker"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/cdf-route-tracker/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/cdf-route-tracker",
        "tsConfig": "libs/shared/cdf-route-tracker/tsconfig.lib.json",
        "project": "libs/shared/cdf-route-tracker/package.json",
        "main": "libs/shared/cdf-route-tracker/src/index.ts",
        "generatePackageJson": false,
        "extractLicenses": true,
        "sourceMap": false,
        "vendorChunk": false,
        "optimization": false,
        "commonChunk": false,
        "runtimeChunk": false,
        "compiler": "tsc",
        "assets": [],
        "webpackConfig": "libs/shared/cdf-route-tracker/webpack.config.js"
      },
      "configurations": {
        "single-spa": {
          "fileReplacements": [
            {
              "replace": "libs/shared/cdf-route-tracker/src/set-public-path.ts",
              "with": "libs/shared/cdf-route-tracker/src/environments/single-spa/set-public-path.ts"
            }
          ]
        },
        "production": {
          // "generatePackageJson": true
        }
      },
      "defaultConfiguration": "production"
    },
    "build-single-spa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nx build cdf-route-tracker --configuration=single-spa"
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "staticFilePath": "dist/libs/shared/cdf-route-tracker",
        "buildTarget": "cdf-route-tracker:build",
        "port": 3009
      },
      "configurations": {
        "production": {
          "buildTarget": "cdf-route-tracker:build-:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/cdf-route-tracker/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
