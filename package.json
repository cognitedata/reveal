    "testcafe": "^1.14.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "node_modules",
      "__tests__",
      ".stories."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
