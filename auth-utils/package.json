  "description": "The auth layer of all cognite react apps",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jest",
