    "platypus": {
      "root": "apps/platypus",
      "sourceRoot": "apps/platypus/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "build",
            "index": "apps/platypus/src/index.html",
            "main": "apps/platypus/src/main.tsx",
            "polyfills": "apps/platypus/src/polyfills.ts",
            "tsConfig": "apps/platypus/tsconfig.app.json",
            "assets": [
              "apps/platypus/src/favicon.ico",
              "apps/platypus/src/manifest.json",
              "apps/platypus/src/assets"
            ],
            "styles": [
              "apps/platypus/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "preview": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.preview.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            },

            "mock": {
              "fileReplacements": [
                {
                "replace": "apps/platypus/src/environments/environment.ts",
                "with": "apps/platypus/src/environments/environment.mock.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": false,
              "namedChunks": false,
              "extractLicenses": false,
              "vendorChunk": false
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            }
          }
        },
        "postbuild": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "mkdir -p scripts/js",
              "cp *.map scripts/js"
            ],
            "cwd": "build",
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "platypus:build",
            "hmr": true
          },
          "configurations": {
            "mock": {
              "buildTarget": "platypus:build:mock",
              "hmr": true,
              "proxyConfig": "apps/platypus/proxy.conf.json"
            },
            "preview": {
              "buildTarget": "platypus:build:preview",
              "hmr": false
            },
            "staging": {
              "buildTarget": "platypus:build:staging",
              "hmr": false
            },
            "production": {
              "buildTarget": "platypus:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/platypus/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/platypus"
          ],
          "options": {
            "jestConfig": "apps/platypus/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/platypus/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "storybook-static",
            "config": {
              "configFolder": "apps/platypus/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
