{
  "name": "@cognite/subsurface-visualizer",
  "version": "0.0.64",
  "description": "Subsurface visualizer for cognite",
  "main": "subsurface-visualizer.js",
  "module": "subsurface-visualizer.js",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "types"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "build:types": "tsc --emitDeclarationOnly",
    "build-dev": "webpack --debug",
    "build:css": "node-sass src/UserInterface/styles/scss -o src/UserInterface/styles/css ",
    "watch:css": "npm run build:css && node-sass src/UserInterface/styles/scss -o src/UserInterface/styles/css -w -r",
    "start:react": "webpack-dev-server --mode development --open --hot --config webpack.config.dev.js",
    "start-dev": "npm-run-all -p watch:css start:react",
    "clean": "tsc --build --clean",
    "serve": "webpack-dev-server --debug --color --progress --watch",
    "lint": "tslint --project .",
    "predeploy": "cd standalone && npm install && npm run build",
    "deploy": "gh-pages -d standalone/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitedata/subsurface-visualization.git"
  },
  "keywords": [
    "subsurface",
    "cognite",
    "subsurface-visualization",
    "react",
    "subsurface-visualizer"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/cognitedata/subsurface-visualization/issues"
  },
  "homepage": "https://github.com/cognitedata/subsurface-visualization#readme",
