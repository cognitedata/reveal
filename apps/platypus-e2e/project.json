{
  "name": "platypus-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/platypus-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/platypus-e2e/cypress.config.js",
        "devServerTarget": "platypus:serve:mock"
      },
      "configurations": {
        "production": {
          "devServerTarget": "platypus:serve:production"
        },
        "mockV2": {
          "devServerTarget": "platypus:serve:mock"
        },
        "mockV3": {
          "env": {
            "FDM_VERSION": "3"
          },
          "ignoreTestFiles": "**/@(ui-editor.cy.ts|manual-data-ingestion.cy.ts|suggestions.cy.ts)"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/platypus-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["platypus"]
}
