{
  "name": "platypus",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/platypus/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/platypus",
        "index": "apps/platypus/src/index.html",
        "main": "apps/platypus/src/bootstrap.tsx",
        "runtimeChunk": false,
        "vendorChunk": false,
        "commonChunk": false,
        "tsConfig": "apps/platypus/tsconfig.app.json",
        "assets": ["apps/platypus/src/favicon.ico", "apps/platypus/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/platypus/webpack-single-spa-config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "preview": {
          "fileReplacements": [
            {
              "replace": "apps/platypus/src/environments/environment.ts",
              "with": "apps/platypus/src/environments/preview/environment.preview.ts"
            },
            {
              "replace": "apps/platypus/src/bootstrap.tsx",
              "with": "apps/platypus/src/environments/preview/bootstrap.tsx"
            },
            {
              "replace": "apps/platypus/src/environments/AppContainer.tsx",
              "with": "apps/platypus/src/environments/preview/AppContainer.tsx"
            },
            {
              "replace": "apps/platypus/src/environments/cogniteSdk.ts",
              "with": "apps/platypus/src/environments/preview/cogniteSdk.ts"
            },
            {
              "replace": "apps/platypus/src/app/Routes.tsx",
              "with": "apps/platypus/src/environments/preview/Routes.tsx"
            },
            {
              "replace": "apps/platypus/src/environments/useTranslateProvider.tsx",
              "with": "apps/platypus/src/environments/preview/useTranslateProvider.tsx"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "runtimeChunk": false,
          "main": "apps/platypus/src/main.tsx",
          "budgets": [],
          "webpackConfig": "apps/platypus/webpack-module-federation-config.js"
        },
        "mock": {
          "fileReplacements": [
            {
              "replace": "apps/platypus/src/environments/environment.ts",
              "with": "apps/platypus/src/environments/mock/environment.mock.ts"
            },
            {
              "replace": "apps/platypus/src/environments/AppContainer.tsx",
              "with": "apps/platypus/src/environments/mock/AppContainer.tsx"
            },
            {
              "replace": "apps/platypus/src/environments/cogniteSdk.ts",
              "with": "apps/platypus/src/environments/mock/cogniteSdk.ts"
            },
            {
              "replace": "apps/platypus/src/environments/useFlag.ts",
              "with": "apps/platypus/src/environments/mock/useFlag.ts"
            },
            {
              "replace": "apps/platypus/src/app/SubAppContainer.tsx",
              "with": "apps/platypus/src/environments/mock/SubAppContainer.tsx"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false
        },
        "fusion": {
          "fileReplacements": [
            {
              "replace": "apps/platypus/src/environments/cogniteSdk.ts",
              "with": "apps/platypus/src/environments/fusion/cogniteSdk.ts"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/platypus/src/environments/cogniteSdk.ts",
              "with": "apps/platypus/src/environments/fusion/cogniteSdk.ts"
            },
            {
              "replace": "apps/platypus/src/environments/environment.ts",
              "with": "apps/platypus/src/environments/environment.prod.ts"
            }
          ],
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": []
        }
      },
      "defaultConfiguration": "fusion"
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "platypus:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "platypus:build:development",
          "hmr": true
        },
        "mock": {
          "buildTarget": "platypus:build:mock",
          "hmr": true,
          "proxyConfig": "apps/platypus/proxy.conf.json",
          "allowedHosts": ".dev.fusion.cogniteapp.com,apps-staging.cognite.com,localhost:*"
        },
        "fusion": {
          "buildTarget": "platypus:build:fusion",
          "hmr": true,
          "proxyConfig": "apps/platypus/proxy.conf.json",
          "allowedHosts": ".dev.fusion.cogniteapp.com,apps-staging.cognite.com,all",
          "ssl": true
        },
        "preview": {
          "buildTarget": "platypus:build:preview",
          "hmr": false,
          "allowedHosts": ".dev.fusion.cogniteapp.com,apps-staging.cognite.com,localhost:*",
          "ssl": true
        },
        "production": {
          "buildTarget": "platypus:build:production",
          "hmr": false,
          "ssl": true
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/platypus/src/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/platypus"],
      "options": {
        "jestConfig": "apps/platypus/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "apps/platypus/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "configDir": "apps/platypus/.storybook",
        "outputDir": "dist/storybook/platypus"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": [],
  "pipeline": {
    "releaseStrategy": "multi-branch",
    "previewPackageName": "@cognite/cdf-solutions-ui",
    "i18n": true
  }
}
