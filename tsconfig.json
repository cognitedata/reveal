{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": true,
    "declarationDir": "dist",
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["es2015", "dom"],
    "module": "es2015",
    "moduleResolution": "node",
    "outDir": "dist",
    "resolveJsonModule": true,
    "sourceMap": false,
    "strictPropertyInitialization": true,
    "target": "es5"
  }
}
