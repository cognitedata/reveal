{
  "name": "notebook",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/notebook/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/notebook",
        "index": "apps/notebook/src/index.html",
        "baseHref": "/",
        "main": "apps/notebook/src/main.tsx",
        "tsConfig": "apps/notebook/tsconfig.app.json",
        "assets": [],
        "styles": [],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/notebook/webpack-config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": false
        },
        "staging": {
          "fileReplacements": [
            {
              "replace": "apps/notebook/src/environments/environment.ts",
              "with": "apps/notebook/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "preview": {
          "fileReplacements": [
            {
              "replace": "apps/notebook/src/environments/environment.ts",
              "with": "apps/notebook/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/notebook/src/environments/environment.ts",
              "with": "apps/notebook/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "notebook:build",
        "hmr": true,
        "port": 3015,
        "ssl": true,
        "allowedHosts": ".dev.fusion.cogniteapp.com"
      },
      "configurations": {
        "development": {
          "buildTarget": "notebook:build:development"
        },
        "production": {
          "buildTarget": "notebook:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/notebook/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "notebook:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/notebook/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [],
  "pipeline": {
    "previewPackageName": "@cognite/cdf-ui-notebook"
  }
}
