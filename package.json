    "@cognite/reveal": "^2.1.2",
    "@cognite/sdk": "^6.3.1",
    "@cognite/sdk-provider": "^1.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.2",
    "@types/react-datepicker": "^3.1.7",
    "@types/react-highlight-words": "^0.16.3",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "antd": "^4.20.1",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.41.0",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-datepicker": "^3.3.0",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-query": "^3.16.1",
    "react-router-dom": "^6.2.2",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.7",
    "single-spa": "^5.8.3",
    "single-spa-react": "^4.1.1",
    "styled-components": "^5.3.1",
    "systemjs-webpack-interop": "^2.3.6",
    "use-debounce": "^6.0.1"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "json"
    ]
  },
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=3010 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3010 react-app-rewired --max_old_space_size=8192 start",
    "build": "react-app-rewired --max_old_space_size=8192 build",
    "clean": "rm -rf dist/",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-data-exploration --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "react-app-rewired test ",
    "test:once": "react-app-rewired test --watchAll=false",
    "test:debugger": "react-app-rewired --inspect-brk test --runInBand --no-cache --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "ci": "yarn lint --max-warnings 0",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "tsc:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "deploy": "gsutil -m cp -r build/. gs://cdf-hub-dev",
    "e2e:dev": "concurrently --kill-others 'yarn start' 'yarn cy:open' "
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.0.0",
    "@cognite/eslint-config": "^1.6.1",
    "@cognite/eslint-plugin": "^1.4.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/react": "^11.2.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/mixpanel-browser": "^2.35.4",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/sinon": "^9.0.10",
    "@types/styled-components": "^5.1.3",
    "babel-loader": "~8.1.0",
    "commander": "^6.1.0",
    "concurrently": "^7.1.0",
    "customize-cra": "^1.0.0",
    "cypress": "^9.6.0",
    "dotenv": "^16.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "~6.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.22",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "postcss-prefixwrap": "^1.19.0",
    "prettier": ">=2.1.2",
    "react-app-rewired": "^2.2.1",
    "react-is": "^16.13.1",
    "react-scripts": "^4.0.3",
    "sinon": "^9.2.3",
    "style-loader": "^1.3.0",
    "typescript": "~4.4.0",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
