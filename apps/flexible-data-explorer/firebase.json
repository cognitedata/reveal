{
  "hosting": {
    "public": ".",
    "trailingSlash": false,
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "redirects": [
      {
        "source": "/",
        "destination": "/explore",
        "type": 302
      }
    ],
    "rewrites": [
      {
        "source": "/explore/**",
        "destination": "/explore/index.html"
      }
    ]
  }
}
