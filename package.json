{
  "name": "@cognite/cognite-charts",
  "version": "0.30.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:fusion": "react-app-rewired --max_old_space_size=4096 build",
    "build-storybook": "build-storybook -s public",
    "chromatic": "chromatic --exit-zero-on-changes",
    "cy": "cypress open",
    "cy:ci": "cypress run",
    "dedup": "yarn-deduplicate",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' 'public/index.html'",
    "lint:fix": "yarn lint --fix",
    "postinstall": "yarn dedup",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,jsx,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "start": "react-scripts start",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test --watchAll=false",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook --coverage && npx nyc report --reporter=json --report-dir=./coverage/storybook\"",
    "test:ci": "react-scripts test --ci --silent --coverage --reporters=default --reporters=jest-junit",
    "test:watch": "./scripts/test.sh"
  },
  "dependencies": {
