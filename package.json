    "@cognite/react-feature-flags": "^0.2.1",
    "@cognite/sdk": "7.12.0",
    "@cognite/sdk-provider": "^2.0.6",
    "@cognite/sdk-react-query-hooks": "^6.0.3",
    "@cognite/z-index": "^0.2.0",
    "@google-cloud/storage": "^5.7.2",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/enzyme": "^3.10.12",
    "@types/lodash": "^4.14.168",
    "@types/mime-types": "^2.1.0",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/uuid": "^8.3.0",
    "antd": "^4.15.2",
    "chart.js": "^3.4.1",
    "commander": "^5.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.29.3",
    "enzyme": "^3.11.0",
    "fs-extra": "^9.0.1",
    "i18next": "^19.8.2",
    "i18next-chained-backend": "^2.0.1",
    "i18next-http-backend": "^1.0.21",
    "i18next-localstorage-backend": "^3.1.2",
    "i18next-locize-backend": "^4.1.8",
    "i18next-pseudo": "^2.2.0",
    "path-to-regexp": "^6.2.0",
    "query-string": "^6.13.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-i18next": "^11.8.5",
    "react-query": "^3.16.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "single-spa-react": "^3.2.0",
    "styled-components": "^5.3.6",
