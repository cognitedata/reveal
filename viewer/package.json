{
  "name": "@cognite/reveal",
  "version": "3.0.0-beta.1",
  "private": true,
  "description": "WebGL based 3D viewer for CAD and point clouds processed in Cognite Data Fusion.",
  "homepage": "https://github.com/cognitedata/reveal/tree/master/viewer",
  "repository": {
    "type": "git",
    "url": "https://github.com/cognitedata/reveal.git",
    "directory": "viewer"
  },
  "license": "Apache-2.0",
  "author": "Lars Moastuen <lars.moastuen@cognite.com>",
  "contributors": [
    "Joachim Bj√∏rne <joachim.bjorne@cognite.com>",
    "Christopher Tannum <christopher.tannum@cognite.com>",
    "Maksim Nesterenko <maksim.nesterenko@cognite.com>"
  ],
  "sideEffects": false,
  "main": "index.js",
  "types": "index.d.ts",
  "workspaces": [
    "core",
    "packages/*"
  ],
  "scripts": {
    "build": "yarn run clean && webpack --env.development && tsc-alias -p tsconfig.webpack.json",
    "build:watch": "yarn run clean && webpack --env.development --watch",
    "build:prod": "yarn run clean && webpack --env.production && tsc-alias -p tsconfig.webpack.json",
    "mkdist": "shx mkdir -p dist && shx cp README.md LICENSE dist",
    "clean": "shx rm -rf dist/ && yarn run mkdist",
    "test": "jest --maxWorkers=4",
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.js --max-warnings 0 --cache",
    "prebump": "yarn version --no-git-tag-version && yarn && cd ../documentation && yarn replace-latest-by-next",
    "bump": " yarn bump:commit && yarn bump:tag && yarn bump:push",
    "bump:commit": "git add -A && cross-var git commit -m \"chore(viewer): bump version $npm_package_version\"",
    "bump:tag": "cross-var git tag $npm_package_name@$npm_package_version",
    "bump:push": "git push -u origin head && git push --tags",
    "release": "yarn build:prod && cd dist && yarn publish --non-interactive",
    "ws:start": "cd $INIT_CWD && webpack-dev-server --open --env.dir=$INIT_CWD",
    "ws:test": "cd $INIT_CWD && jest --runInBand",
    "ws:lint": "cd $INIT_CWD && eslint . --ext .ts,.js --max-warnings 0 --cache"
  },
  "dependencies": {
    "@cognite/potree-core": "1.5.1",
    "@cognite/reveal-parser-worker": "1.3.0",
    "@tweenjs/tween.js": "18.6.4",
    "@types/draco3dgltf": "1.4.0",
    "@types/three": "0.137.0",
    "assert": "2.0.0",
    "comlink": "4.3.1",
    "geo-three": "0.0.16",
    "glslify": "7.1.1",
    "glslify-import": "3.1.0",
    "lodash": "4.17.21",
    "loglevel": "1.8.0",
    "mixpanel-browser": "2.45.0",
    "rxjs": "7.5.5",
    "skmeans": "0.11.3",
    "three": "0.137.5"
  },
  "devDependencies": {
    "@azure/msal-browser": "2.22.1",
    "@cognite/sdk": "7.1.1",
    "@cognite/sdk-core": "^4.1.0",
    "@types/dat.gui": "0.7.7",
    "@types/jest": "26.0.24",
    "@types/jsdom": "16.2.14",
    "@types/lodash": "4.14.180",
    "@types/mixpanel-browser": "2.38.0",
    "@types/node": "16.11.26",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/random-seed": "0.3.3",
    "@types/skmeans": "0.11.3",
    "@types/tween.js": "18.5.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "copy-pkg-json-webpack-plugin": "0.0.40",
    "copy-webpack-plugin": "10.2.4",
    "core-js": "3.21.1",
    "cross-var": "1.1.0",
    "dat.gui": "0.7.9",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-jsdoc": "38.0.3",
    "eslint-plugin-lodash": "7.4.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "file-loader": "6.2.0",
    "gl": "5.0.0",
    "glslify-loader": "2.0.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "jest-extended": "0.11.5",
    "jsdom": "16.7.0",
    "lint-staged": "12.3.7",
    "moq.ts": "8.0.3",
    "nock": "13.2.4",
    "prettier": "2.6.0",
    "random-seed": "0.3.0",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "run-script-os": "1.1.6",
    "shx": "0.3.4",
    "ts-jest": "26.5.6",
    "ts-loader": "8.3.0",
    "tsc-alias": "1.6.4",
    "typescript": "4.5.5",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.3",
    "webpack-log": "3.0.2",
    "webpack-node-externals": "2.5.2",
    "whatwg-fetch": "3.6.2",
    "worker-plugin": "5.0.1"
  },
  "peerDependencies": {
    "@cognite/sdk": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "@cognite/sdk-core": "^3.0.0"
  },
  "glslify": {
    "transform": [
      "glslify-import"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint"
    }
  }
}
