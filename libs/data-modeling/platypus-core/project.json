{
  "name": "data-modeling-platypus-core",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-modeling/platypus-core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": ".",
        "outputPath": "dist/libs/@platypus/platypus-core",
        "tsConfig": "libs/data-modeling/platypus-core/tsconfig.lib.json",
        "project": "libs/data-modeling/platypus-core/package.json",
        "entryFile": "libs/data-modeling/platypus-core/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "libs/data-modeling/platypus-core/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-modeling/platypus-core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/data-modeling/platypus-core"],
      "options": {
        "jestConfig": "libs/data-modeling/platypus-core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
