{
  "root": "apps/platypus-e2e",
  "sourceRoot": "apps/platypus-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/platypus-e2e/cypress.config.js",
        "devServerTarget": "platypus:serve:mock"
      },
      "configurations": {
        "production": {
          "devServerTarget": "platypus:serve:production"
        },
        "mockV2": {
          "devServerTarget": "platypus:serve:mock"
        },
        "mockV3": {
          "env": {
            "FDM_VERSION": "3"
          },
          "ignoreTestFiles": "**/@(suggestions.cy.ts|data-preview-page.cy.ts|publish-new-schema.cy.ts|preview-data-model.cy.ts|local-drafts.cy.ts|manual-data-ingestion.cy.ts)"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/platypus-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["platypus"]
}
