    "@testing-library/react": "^13.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.177",
    "@types/node": "^18.0.0",
    "@types/papaparse": "^5.3.1",
    "@types/react": "^18",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5.3.1",
    "@types/single-spa-react": "^2.12.0",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^9.0.0",
