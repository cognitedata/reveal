{
  "name": "industry-canvas-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/industry-canvas-ui/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/industry-canvas-ui",
        "index": "apps/industry-canvas-ui/src/index.html",
        "main": "apps/industry-canvas-ui/src/index.tsx",
        "runtimeChunk": false,
        "vendorChunk": false,
        "commonChunk": false,
        "tsConfig": "apps/industry-canvas-ui/tsconfig.app.json",
        "assets": ["apps/industry-canvas-ui/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/industry-canvas-ui/webpack-config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "preview": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "production": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": []
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "industry-canvas-ui:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "industry-canvas-ui:build:development",
          "hmr": true,
          "liveReload": true,
          "allowedHosts": ".dev.fusion.cogniteapp.com,apps-staging.cognite.com"
        },
        "preview": {
          "buildTarget": "industry-canvas-ui:build:preview",
          "hmr": false
        },
        "production": {
          "buildTarget": "industry-canvas-ui:build:production",
          "hmr": false
        }
      },
      "defaultConfiguration": "development"
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "industry-canvas-ui:build",
        "port": 3010
      },
      "configurations": {
        "development": {
          "buildTarget": "industry-canvas-ui:build:development"
        },
        "preview": {
          "buildTarget": "industry-canvas-ui:build:preview"
        },
        "production": {
          "buildTarget": "industry-canvas-ui:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/industry-canvas-ui/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": [],
  "pipeline": {
    "previewPackageName": "@cognite/cdf-industry-canvas-ui"
  }
}
