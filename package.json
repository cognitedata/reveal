{
  "name": "@cognite/react-demo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@cognite/cogs.js": "^1.3.0",
    "@cognite/metrics": "^0.0.32",
    "@sentry/browser": "^5.19.0",
    "antd": "^4.4.0",
    "i18next": "^19.5.2",
    "i18next-chained-backend": "^2.0.1",
    "i18next-http-backend": "^1.0.16",
    "i18next-localstorage-backend": "^3.1.1",
    "i18next-locize-backend": "^4.1.0",
    "i18next-pseudo": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@cognite/eslint-config": "^1.5.0",
    "@cognite/eslint-plugin": "^1.0.17",
    "@storybook/preset-create-react-app": "^3.1.2",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/sinon": "^9.0.4",
    "@types/styled-components": "^5.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^3.3.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sinon": "^9.0.2",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
