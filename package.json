{
  "name": "@cognite/cdf-extractor-downloads",
  "version": "0.8.0",
  "private": true,
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=8000 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=8000 rescripts start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-extractor-downloads -o index.js -d ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test --passWithNoTests",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)"
  },
  "dependencies": {
    "@cognite/cdf-utilities": "^0.10.1",
    "@cognite/cogs.js": "^5.2.1",
    "@cognite/metrics": "^0.1.7",
    "@cognite/react-feature-flags": "^1.0.0",
    "@cognite/sdk": "^6.3.1",
    "@cognite/sdk-provider": "^1.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.2",
    "@rescripts/cli": "^0.0.14",
    "@rescripts/utilities": "^0.0.8",
    "@sentry/browser": "^5.29.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "antd": "^4.15.2",
    "query-string": "^6.13.6",
    "react": "^17.0.2",
    "react-base-table": "^1.13.0",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.15.6",
    "react-iframe": "^1.8.0",
    "react-query": "^3.25.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "single-spa-react": "^4.6.0",
    "styled-components": "^5.2.1",
    "systemjs-webpack-interop": "^2.1.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.16.0",
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.1.1",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/rescript-use-babel-config": "^0.0.10",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^14.14.20",
    "@types/papaparse": "^5.3.1",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.3.1",
    "@types/single-spa-react": "^2.12.0",
    "@types/sinon": "^9.0.10",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^3.3.2",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^4.0.0",
