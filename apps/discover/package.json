{
  "name": "discover",
  "description": "Discover - AKA: PrettyPoly",
  "version": "1.29.1",
  "private": true,
  "scripts": {
    "analyse": "../../node_modules/.bin/source-map-explorer 'build/static/js/*.js' --only-mapped --html /tmp/result.html && google-chrome /tmp/result.html",
    "analyze": "yarn analyse",
    "build": "bazel build :build_sed",
    "clean": "rm -Rf build",
    "eslint": "bazel test :eslint_test",
    "eslint:fix": "bazel run :eslint_test -- --fix",
    "lint": "bazel test :eslint_test :prettier_test",
    "lint:fast": "pretty-quick --staged",
    "lint:old": "scripts/lint.sh",
    "prettier": "bazel test :prettier_test",
    "prettier:fix": "bazel run :prettier_test -- --write",
    "start": "./scripts/start.sh",
    "start:a": "REACT_APP_CLUSTER=azure-dev ./scripts/start.sh",
    "start:bp": "REACT_APP_CLUSTER=bp-northeurope ./scripts/start.sh",
    "start:idp": "./scripts/startIdP.sh",
    "storybook": "ibazel run :start-storybook",
    "build-storybook": "bazel build :storybook-static",
    "test": "bazel run :jest -- ",
    "test:debug": "yarn test --runInBand",
    "test:coverage": "yarn test --collectCoverage",
    "update-helpers": "bazel run :generate_package_json_helpers",
    "cypress:run:dev": "ibazel run :cypress_run -- --env REACT_APP_E2E_USER=$(git config user.email)",
    "cypress:run:e2e": "ibazel run :cypress_run -- --env REACT_APP_E2E_USER=$(git config user.email),USER_PREFIX=e2e,PROJECT=discover-e2e-bluefield",
    "cypress:ci": "bazel test :cypress_test",
    "cypress:nightly": "bazel test :cypress_nightly_test",
    "cypress:open": "../../node_modules/.bin/cypress open --env REACT_APP_E2E_USER=$(git config user.email)",
    "cypress:open:e2e": "../../node_modules/.bin/cypress open --env REACT_APP_E2E_USER=$(git config user.email),USER_PREFIX=e2e,PROJECT=discover-e2e-bluefield"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
