{
  "name": "extraction-pipelines",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/extraction-pipelines/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/extraction-pipelines",
        "index": "apps/extraction-pipelines/src/index.html",
        "main": "apps/extraction-pipelines/src/index.tsx",
        "runtimeChunk": false,
        "vendorChunk": false,
        "commonChunk": false,
        "tsConfig": "apps/extraction-pipelines/tsconfig.app.json",
        "assets": [],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/extraction-pipelines/webpack-config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "preview": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "staging": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": []
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": []
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "extraction-pipelines:build",
        "hmr": true,
        "port": 3033,
        "ssl": true,
        "allowedHosts": ".dev.fusion.cogniteapp.com,apps-staging.cognite.com"
      },
      "configurations": {
        "development": {
          "buildTarget": "extraction-pipelines:build:development",
          "hmr": true,
          "liveReload": true
        },
        "preview": {
          "buildTarget": "extraction-pipelines:build:preview",
          "hmr": false
        },
        "production": {
          "buildTarget": "extraction-pipelines:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/extraction-pipelines/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/extraction-pipelines/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/extraction-pipelines/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "extraction-pipelines:serve",
        "port": 3001,
        "env": {
          "ORG": "cog-e2e",
          "PROJECT": "e2e-greenfield",
          "CLUSTER": "greenfield.cognitedata.com",
          "IDP_INTERNAL_ID": "09f375ff-13b6-4d58-95fd-869b098d4808"
        }
      },
      "configurations": {
        "production": {
          "devServerTarget": "extraction-pipelines:serve:production"
        },
        "e2e": {
          "devServerTarget": "extraction-pipelines:serve:production"
        }
      },
      "defaultConfiguration": "e2e"
    }
  },
  "tags": [],
  "pipeline": {
    "releaseStrategy": "single-branch",
    "previewPackageName": "@cognite/cdf-integrations-ui"
  }
}
