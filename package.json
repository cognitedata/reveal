{
  "name": "@cognite/cdf-raw-explorer",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "start": "env GENERATE_SOURCEMAP=false BROWSER=none WDS_SOCKET_PORT=3018 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3018 react-app-rewired start",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-raw-explorer --outputFilename index.js --destinationPath ./build",
