{
  "name": "cdf-document-search",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cdf-document-search/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/cdf-document-search",
        "index": "apps/cdf-document-search/src/index.html",
        "baseHref": "/",
        "main": "apps/cdf-document-search/src/index.tsx",
        "tsConfig": "apps/cdf-document-search/tsconfig.app.json",
        "assets": [
          "apps/cdf-document-search/src/favicon.ico",
          "apps/cdf-document-search/src/assets"
        ],
        "styles": ["apps/cdf-document-search/src/styles.css"],
        "scripts": [],
        "isolatedConfig": true,
        "webpackConfig": "apps/cdf-document-search/webpack.config.js"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/cdf-document-search/src/environments/environment.ts",
              "with": "apps/cdf-document-search/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "staging": {
          "fileReplacements": [
            {
              "replace": "apps/cdf-document-search/src/environments/environment.ts",
              "with": "apps/cdf-document-search/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "preview": {
          "fileReplacements": [
            {
              "replace": "apps/cdf-document-search/src/environments/environment.ts",
              "with": "apps/cdf-document-search/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cdf-document-search:build",
        "hmr": true,
        "allowedHosts": "all",
        "port": 3016,
        "ssl": true,
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      },
      "configurations": {
        "development": {
          "buildTarget": "cdf-document-search:build:development"
        },
        "production": {
          "buildTarget": "cdf-document-search:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cdf-document-search/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cdf-document-search/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": [],
  "pipeline": {
    "previewPackageName": "@cognite/cdf-document-search-ui"
  }
}
