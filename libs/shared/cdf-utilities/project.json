{
  "name": "cdf-utilities",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/cdf-utilities/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "npm": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "outputPath": "dist/libs/shared/cdf-utilities",
        "commands": ["./scripts/npm.sh"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/cdf-utilities/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "format": ["esm"],
        "outputPath": "dist/libs/shared/cdf-utilities",
        "tsConfig": "libs/shared/cdf-utilities/tsconfig.lib.json",
        "project": "libs/shared/cdf-utilities/package.json",
        "entryFile": "libs/shared/cdf-utilities/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "glob": "libs/shared/cdf-utilities/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/cdf-utilities/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "configDir": "libs/shared/cdf-utilities/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "uiFramework": "@storybook/react",
        "configDir": "libs/shared/cdf-utilities/.storybook",
        "outputDir": "storybook-static"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
