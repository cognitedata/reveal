{
  "name": "@cognite/cdf-vision-subapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none WDS_SOCKET_PORT=8000 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=8000 HTTPS=true rescripts start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-vision-subapp -o index.js -d ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)"
  },
  "dependencies": {
    "@cognite/cdf-sdk-singleton": "^0.6.1",
    "@cognite/cdf-utilities": "^0.9.0",
    "@cognite/cogs.js": "^2.15.1",
    "@cognite/data-exploration": "^0.5.3",
    "@cognite/metrics": "^0.1.7",
    "@cognite/react-picture-annotation": "^1.11.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/browser": "^6.2.0",
    "@types/react-redux": "^7.1.16",
    "antd": "^4.12.2",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-base-table": "^1.12.0",
    "react-dom": "^16.14.0",
    "react-query": "^3.9.9",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "single-spa-react": "^3.1.0",
    "styled-components": "^5.2.1",
    "systemjs-webpack-interop": "^2.3.5"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.0.0",
    "@cognite/eslint-config": "^1.6.1",
    "@cognite/eslint-plugin": "^1.3.8",
    "@google-cloud/storage": "^5.7.2",
    "@keenondrums/singleton": "^1.0.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/cli": "^0.0.15",
    "@rescripts/rescript-use-babel-config": "^0.0.12",
    "@rescripts/utilities": "^0.0.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.28",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/single-spa-react": "^3.0.1",
    "@types/styled-components": "^5.1.7",
    "commander": "^7.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "cross-var": "^1.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "fs-extra": "^9.1.0",
    "husky": "^5.0.9",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "postcss-prefixwrap": "^1.22.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
