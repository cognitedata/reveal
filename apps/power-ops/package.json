{
  "name": "@cognite/power-ops",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "start:build": "bazel run :serve_build",
    "build": "bazel build :build_sed",
    "build:manual": "./script/build.sh",
    "test": "bazel run :jest -- ",
    "test:debug": "yarn test --runInBand",
    "test:manual": "./scripts/test.sh",
    "lint:bazel": "bazel test :eslint_test :prettier_test",
    "eslint:bazel": "bazel test :eslint_test",
    "eslint:bazel-fix": "bazel run :eslint_test -- --fix",
    "prettier:bazel": "bazel test :prettier_test",
    "prettier:bazel-fix": "bazel run :prettier_test -- --write",
    "storybook": "bazel run :start-storybook",
    "build-storybook": "bazel build :storybook-static",
    "e2e:start-idp": "./scripts/start-fake-idp.sh",
    "cypress:run-live": "./scripts/cypress-run-live.sh",
    "cypress:bazel-run-live": "ibazel run :cypress",
    "update-helpers": "bazel run :generate_package_json_helpers",
    "deploy": "cd ../.. && ./scripts/deploy.sh power-ops",
    "update-json-schema": "npx ts-json-schema-generator --path './src/types/index.ts' --type 'WorkflowSchemaEditable' --tsconfig 'tsconfig.json' -o './src/types/WorkflowSchemaEditable.schema.json'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "react-table": "^7.7.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
