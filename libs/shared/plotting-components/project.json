{
  "name": "shared-plotting-components",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/plotting-components/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "npm": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "options": {
        "outputPath": "dist/libs/shared/plotting-components",
        "commands": [
          "./scripts/npm.sh"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/shared/plotting-components/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "format": [
          "esm"
        ],
        "external": [
          "react/jsx-runtime"
        ],
        "outputPath": "dist/libs/shared/plotting-components",
        "tsConfig": "libs/shared/plotting-components/tsconfig.lib.json",
        "project": "libs/shared/plotting-components/package.json",
        "entryFile": "libs/shared/plotting-components/src/index.ts",
        "rollupConfig": "libs/shared/plotting-components/rollupConf.js",
        "compiler": "babel",
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "glob": "libs/shared/plotting-components/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/shared/plotting-components/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "configDir": "libs/shared/plotting-components/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": [
        "{options.outputDir}"
      ],
      "options": {
        "uiFramework": "@storybook/react",
        "configDir": "libs/shared/plotting-components/.storybook",
        "outputDir": "storybook-static"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}