{
  "name": "data-exploration-domain-layer",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-exploration/domain-layer/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "compiler": "swc",
      "format": ["esm"],
      "options": {
        "outputPath": "dist/libs/data-exploration/domain-layer",
        "main": "libs/data-exploration/domain-layer/src/index.ts",
        "tsConfig": "libs/data-exploration/domain-layer/tsconfig.lib.json",
        "assets": ["libs/data-exploration/domain-layer/*.md"],
        "updateBuildableProjectDepsInPackageJson": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-exploration/domain-layer/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/data-exploration/domain-layer/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
