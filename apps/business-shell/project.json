{
  "name": "business-shell",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/business-shell/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/business-shell"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": false
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false
        },
        "mock": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "business-shell:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "business-shell:build:development"
        },
        "production": {
          "buildTarget": "business-shell:build:production",
          "hmr": false
        },
        "mock": {
          "buildTarget": "business-shell:build:mock",
          "hmr": true,
          "port": 3001,
          "ssl": true,
          "proxyConfig": "apps/business-shell/proxy.conf.json"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/business-shell/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/business-shell/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "business-shell:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "business-shell:build:development"
        },
        "production": {
          "buildTarget": "business-shell:build:production"
        }
      }
    }
  },
  "tags": [],
  "pipeline": {
    "releaseStrategy": "single-branch",
    "firebaseProjectId": "fbhosting-217032465111",
    "firebaseSite": "cognite-flexible-data-explorer"
  }
}
