{
  "name": "data-exploration-core",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-exploration/core/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/data-exploration/core",
        "main": "libs/data-exploration/core/src/index.ts",
        "tsConfig": "libs/data-exploration/core/tsconfig.lib.json",
        "assets": [],
        "project": "libs/data-exploration/core/package.json"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-exploration/core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/data-exploration/core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
