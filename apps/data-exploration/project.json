{
  "name": "data-exploration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/data-exploration/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/data-exploration",
        "index": "apps/data-exploration/src/index.html",
        "main": "apps/data-exploration/src/index.tsx",
        "runtimeChunk": false,
        "vendorChunk": false,
        "commonChunk": false,
        "tsConfig": "apps/data-exploration/tsconfig.app.json",
        "assets": ["apps/data-exploration/src/assets"],
        "styles": [],
        "scripts": [],
        "webpackConfig": "apps/data-exploration/webpack-config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": false,
          "namedChunks": false,
          "extractLicenses": false,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "staging": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "preview": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "runtimeChunk": false,
          "budgets": []
        },
        "production": {
          "optimization": false,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": []
        }
      }
    },
    "serve": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "data-exploration:build",
        "hmr": true,
        "allowedHosts": "all",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        },
        "ssl": true,
        "port": 3010
      },
      "configurations": {
        "development": {
          "buildTarget": "data-exploration:build:development"
        },
        "preview": {
          "buildTarget": "data-exploration:build:preview",
          "hmr": false
        },
        "production": {
          "buildTarget": "data-exploration:build:production",
          "hmr": false
        }
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/data-exploration/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "dependsOn": ["^build"],
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/data-exploration"],
      "options": {
        "jestConfig": "apps/data-exploration/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "implicitDependencies": ["data-exploration-e2e"],
  "tags": [],
  "pipeline": {
    "releaseStrategy": "multi-branch",
    "previewPackageName": "@cognite/cdf-data-exploration",
    "i18n": true,
    "trigger": true
  }
}
