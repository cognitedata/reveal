{
  "version": 2,
  "projects": {
    "platypus": {
      "root": "apps/platypus",
      "sourceRoot": "apps/platypus/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "build",
            "index": "apps/platypus/src/index.html",
            "main": "apps/platypus/src/main.tsx",
            "polyfills": "apps/platypus/src/polyfills.ts",
            "tsConfig": "apps/platypus/tsconfig.app.json",
            "assets": [
              "apps/platypus/src/favicon.ico",
              "apps/platypus/src/manifest.json",
              "apps/platypus/src/assets"
            ],
            "styles": ["apps/platypus/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "preview": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.preview.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/platypus/src/environments/environment.ts",
                  "with": "apps/platypus/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "3mb"
                }
              ]
            }
          }
        },
        "postbuild": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["mkdir -p scripts/js", "cp *.map scripts/js"],
            "cwd": "build",
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "platypus:build",
            "hmr": true
          },
          "configurations": {
            "preview": {
              "buildTarget": "platypus:build:preview",
              "hmr": false
            },
            "staging": {
              "buildTarget": "platypus:build:staging",
              "hmr": false
            },
            "production": {
              "buildTarget": "platypus:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/platypus/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/platypus"],
          "options": {
            "jestConfig": "apps/platypus/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/platypus/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "storybook-static",
            "config": {
              "configFolder": "apps/platypus/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
    "platypus-cdf-cli": {
      "root": "apps/platypus-cdf-cli",
      "sourceRoot": "apps/platypus-cdf-cli/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/platypus-cdf-cli",
            "main": "apps/platypus-cdf-cli/src/main.ts",
            "tsConfig": "apps/platypus-cdf-cli/tsconfig.app.json",
            "assets": ["apps/platypus-cdf-cli/src/assets"],
            "generatePackageJson": true,
            "extractLicenses": true,
            "sourceMap": true,
            "webpackConfig": "tools/custom-webpack-configurations/platypus-cli-webpack-config.js"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/platypus-cdf-cli/src/environments/environment.ts",
                  "with": "apps/platypus-cdf-cli/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "platypus-cdf-cli:build",
            "watch": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/platypus-cdf-cli/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/platypus-cdf-cli"],
          "options": {
            "jestConfig": "apps/platypus-cdf-cli/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "platypus-core": {
      "root": "libs/platypus-core",
      "sourceRoot": "libs/platypus-core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/platypus-core/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/platypus-core"],
          "options": {
            "jestConfig": "libs/platypus-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "platypus-infrastructure": {
      "root": "libs/platypus-infrastructure",
      "sourceRoot": "libs/platypus-infrastructure/src",
      "projectType": "library",
      "targets": {

        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/platypus-infrastructure/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/platypus-infrastructure"],
          "options": {
            "jestConfig": "libs/platypus-infrastructure/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "platypus-ui": {
      "root": "libs/platypus-ui",
      "sourceRoot": "libs/platypus-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/platypus-ui/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/platypus-ui"],
          "options": {
            "jestConfig": "libs/platypus-ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    }
  }
}
