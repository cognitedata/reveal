{
  "name": "data-modeling-platypus-common-utils",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-modeling/platypus-common-utils/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "rootDir": ".",
        "format": ["esm", "cjs"],
        "outputPath": "dist/libs/@platypus/platypus-common-utils",
        "tsConfig": "libs/data-modeling/platypus-common-utils/tsconfig.lib.json",
        "project": "libs/data-modeling/platypus-common-utils/package.json",
        "entryFile": "libs/data-modeling/platypus-common-utils/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "tsc",
        "skipTypeField": true,
        "assets": [
          {
            "glob": "libs/data-modeling/platypus-common-utils/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "updateBuildableProjectDepsInPackageJson": false
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/data-modeling/platypus-common-utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/data-modeling/platypus-common-utils"
      ],
      "options": {
        "jestConfig": "libs/data-modeling/platypus-common-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
