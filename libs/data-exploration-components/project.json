{
  "name": "data-exploration-components-old",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/data-exploration-components/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/data-exploration-components",
        "tsConfig": "libs/data-exploration-components/tsconfig.lib.json",
        "entryFile": "libs/data-exploration-components/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "assets": [
          {
            "glob": "libs/data-exploration-components/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/data-exploration-components/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/data-exploration-components"],
      "options": {
        "jestConfig": "libs/data-exploration-components/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "storybook": {
      "executor": "@nrwl/storybook:storybook",
      "options": {
        "uiFramework": "@storybook/react",
        "port": 4400,
        "configDir": "libs/data-exploration-components/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "release": {
      "executor": "@theunderscorer/nx-semantic-release:semantic-release",
      "options": {
        "outputPath": "../../dist/libs/data-exploration-components"
      },
      "dependsOn": ["build"]
    },
    "build-storybook": {
      "executor": "@nrwl/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "uiFramework": "@storybook/react",
        "configDir": "libs/data-exploration-components/.storybook",
        "outputDir": "storybook-static"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  }
}
