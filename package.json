    "react-base-table": "^1.13.0",
    "react-csv": "^2.0.3",
    "react-dom": "^18",
    "react-draggable": "^4.4.4",
    "react-query": "^3.25.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.0",
    "single-spa-react": "^4.6.0",
    "styled-components": "^5.2.1",
