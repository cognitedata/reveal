  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "json"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@cognite/gearbox/dist)/)"
    ]
  },
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=3010 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3010 rescripts --max_old_space_size=8192 start",
    "build": "rescripts --max_old_space_size=8192 build",
    "build-lib": "yarn clean ; NODE_ENV=production rollup -c",
    "clean": "rm -rf dist/",
    "generate-preview-artifacts": "node ./scripts/generate-mfe-artifacts.js -p @cognite/cdf-data-exploration -b cdf-hub-production --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test --testNamePattern '^((?!PERFORMANCE).)*$'",
    "test:once": "yarn test:once:unittests && yarn test:once:performanceTests",
    "test:once:unittests": "rescripts test --watchAll=false --testNamePattern \"^((?!PERFORMANCE).)*$\"",
    "test:once:performanceTests": "rescripts test --watchAll=false --maxWorkers 1 --testNamePattern 'PERFORMANCE'",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "ci": "yarn lint --max-warnings 0",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "gsutil -m cp -r build/. gs://cdf-hub-dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
