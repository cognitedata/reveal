    "i18next-chained-backend": "^2.0.1",
    "i18next-http-backend": "^1.0.17",
    "i18next-localstorage-backend": "^3.1.1",
    "i18next-locize-backend": "^4.1.1",
    "i18next-pseudo": "^2.2.0",
    "lodash": "^4.17.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
