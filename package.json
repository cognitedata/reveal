{
  "name": "@cognite/data-explorer",
  "version": "0.5.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/lib/index.d.ts",
  "license": "UNLICENSED",
  "dependencies": {
    "@babel/runtime": "^7.12.18",
    "@cognite/cdf-utilities": "^0.9.0",
    "@cognite/cogs.js": "^2.12.4",
    "@cognite/data-exploration": "^0.5.7",
    "@cognite/react-feature-flags": "^0.1.0",
    "@cognite/react-picture-annotation": "^1.10.4",
    "@cognite/sdk": "^3.3.2",
    "@cognite/sdk-provider": "^1.0.0",
    "@cognite/sdk-react-query-hooks": "^3.0.0",
    "@sentry/browser": "^6.2.0",
    "@types/react-datepicker": "^3.1.2",
    "antd": "^4.12.3",
    "history": "^4.10.1",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.40.1",
    "query-string": "^6.13.8",
    "react": "^16.14.0",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.14.0",
    "react-query": "^3.9.8",
    "react-router-dom": "^5.2.0",
    "single-spa": "^5.8.3",
    "single-spa-react": "^3.2.0",
    "styled-components": "^5.2.1",
    "systemjs-webpack-interop": "^2.3.5",
    "use-debounce": "^5.2.1"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "json"
    ]
  },
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=3010 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3010 rescripts --max_old_space_size=8192 start",
    "build": "rescripts --max_old_space_size=8192 build",
    "clean": "rm -rf dist/",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-data-exploration --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test ",
    "test:once": "rescripts test --watchAll=false",
    "test:debugger": "rescripts --inspect-brk test --runInBand --no-cache --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "ci": "yarn lint --max-warnings 0",
    "deploy": "gsutil -m cp -r build/. gs://cdf-hub-dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-typescript": "^7.10.4",
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-sdk-singleton": "^0.6.1",
    "@cognite/cdf-ui-cli": "^1.0.0",
    "@cognite/eslint-config": "1.6.1",
    "@cognite/eslint-plugin": "^1.4.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-use-babel-config": "^0.0.10",
    "@rescripts/utilities": "^0.0.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@testing-library/react": "^11.2.2",
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.161",
    "@types/mixpanel-browser": "^2.35.4",
    "@types/node": "^14.11.5",
    "@types/node-cron": "^2.0.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/single-spa-react": "^2.12.0",
    "@types/sinon": "^9.0.10",
    "@types/styled-components": "^5.1.3",
    "babel-loader": "^8.1.0",
    "commander": "^6.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-airbnb": ">=18.2.0",
    "eslint-config-prettier": ">=6",
    "eslint-plugin-import": ">=2.22",
    "eslint-plugin-jest": ">=24.1.0",
    "eslint-plugin-jsx-a11y": ">=6.3",
    "eslint-plugin-prettier": ">=3.1",
    "eslint-plugin-react": ">=7.21.3",
    "eslint-plugin-react-hooks": ">=4.1.2",
    "fs-extra": "^9.0.1",
    "identity-obj-proxy": "^3.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "postcss-prefixwrap": "^1.19.0",
    "prettier": ">=2.1.2",
    "react-is": "^16.13.1",
    "react-scripts": "^3.4.3",
    "rollup": "^2.29.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "sinon": "^9.2.3",
