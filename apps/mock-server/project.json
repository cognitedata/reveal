{
  "name": "mock-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mock-server/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/mock-server",
        "main": "apps/mock-server/src/main.ts",
        "tsConfig": "apps/mock-server/tsconfig.app.json",
        "assets": [
          "apps/mock-server/src/assets",
          "apps/mock-server/src/static",
          "apps/mock-server/src/cert"
        ],
        "generatePackageJson": true,
        "additionalEntryPoints": [
          {
            "entryName": "mock-data",
            "entryPath": "apps/mock-server/src/mock-data.ts"
          }
        ],
        "webpackConfig": "apps/mock-server/webpack-config.js",
        "target": "node",
        "compiler": "tsc"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/mock-server/src/environments/environment.ts",
              "with": "apps/mock-server/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "mock-server:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "mock-server:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/mock-server/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/mock-server"],
      "options": {
        "jestConfig": "apps/mock-server/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
