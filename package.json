{
  "name": "functions-ui",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@cognite/cdf-sdk-singleton": "^0.5.0",
    "@cognite/cdf-utilities": "^0.7.1",
    "@cognite/cogs.js": "^1.18.0",
    "@cognite/gcs-browser-upload": "^1.0.5",
    "@cognite/gearbox": "^2.0.1",
    "@cognite/griff-react": "~0.4.2",
    "@sentry/browser": "^5.21.4",
    "antd": "^3.26.18",
    "cron-validator": "^1.1.1",
    "cronstrue": "^1.96.0",
    "history": "^4.10.1",
    "immer": "^7.0.8",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "mixpanel-browser": "^2.39.0",
    "moment": "^2.27.0",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^0.4.4",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "single-spa": "^5.5.5",
    "single-spa-react": "^3.0.1",
    "styled-components": "^5.1.1",
    "systemjs-webpack-interop": "^2.1.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "html",
      "lcov",
      "json"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@cognite/gearbox/dist)/)"
    ]
  },
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=3011 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3011 rescripts --max_old_space_size=8192 start",
    "build": "rescripts --max_old_space_size=8192 build",
    "generate-preview-artifacts": "node ./scripts/generate-mfe-artifacts.js -p @cognite/cdf-functions-ui -b cdf-hub-production --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test --testNamePattern '^((?!PERFORMANCE).)*$'",
    "test:once": "yarn test:once:unittests && yarn test:once:performanceTests",
    "test:once:unittests": "rescripts test --watchAll=false --testNamePattern \"^((?!PERFORMANCE).)*$\"",
    "test:once:performanceTests": "rescripts test --watchAll=false --maxWorkers 1 --testNamePattern 'PERFORMANCE'",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "ci": "yarn lint --max-warnings 0",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy": "gsutil -m cp -r build/. gs://cdf-hub-dev"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/eslint-config": "1.6.1",
    "@cognite/eslint-plugin": "^1.3.8",
    "@google-cloud/storage": "^5.3.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/cli": "^0.0.14",
    "@rescripts/rescript-use-babel-config": "^0.0.10",
    "@rescripts/utilities": "^0.0.7",
    "@storybook/react": "^6.0.16",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.159",
    "@types/mixpanel-browser": "^2.35.3",
    "@types/node": "^14.6.0",
    "@types/node-cron": "^2.0.3",
    "@types/query-string": "^6.3.0",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-highlight-words": "^0.16.1",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-virtualized": "^9.21.10",
    "@types/redux-mock-store": "^1.0.2",
    "@types/reselect": "^2.2.0",
    "@types/single-spa-react": "^2.12.0",
    "@types/styled-components": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "commander": "^6.0.0",
    "css-loader": "^4.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "eslint-config-airbnb": ">=18.2.0",
    "eslint-config-prettier": ">=6",
    "eslint-plugin-import": ">=2.22",
    "eslint-plugin-jest": ">=23.20",
    "eslint-plugin-jsx-a11y": ">=6.3",
    "eslint-plugin-prettier": ">=3.1",
    "eslint-plugin-react": ">=7.20.6",
    "eslint-plugin-react-hooks": ">=4.1.0",
    "fs-extra": "^9.0.1",
    "identity-obj-proxy": "^3.0.0",
    "less-loader": "^6.2.0",
    "postcss": "^7.0.32",
    "postcss-prefixwrap": "^1.16.0",
    "prettier": ">=2.1.0",
    "react-scripts": "^3.4.3",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
