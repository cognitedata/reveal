{
  "name": "@cognite/cdf-data-exploration",
  "version": "0.16.0",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "typings": "dist/lib/index.d.ts",
  "license": "UNLICENSED",
  "scripts": {
    "start": "env GENERATE_SOURCEMAP=false BROWSER=none WDS_SOCKET_PORT=3010 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3010 react-app-rewired --max_old_space_size=8192 start",
    "build": "react-app-rewired --max_old_space_size=8192 build",
    "clean": "rm -rf dist/",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-data-exploration --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "react-app-rewired test ",
    "test:once": "react-app-rewired test --watchAll=false",
    "test:debugger": "react-app-rewired --inspect-brk test --runInBand --no-cache --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "ci": "yarn lint --max-warnings 0",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "tsc:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "deploy": "gsutil -m cp -r build/. gs://cdf-hub-dev",
    "e2e:dev": "concurrently --kill-others 'yarn start' 'yarn cy:open' "
  },
  "dependencies": {
    "@cognite/cdf-utilities": "^1.14.0",
    "@cognite/cogs.js": "^7.17.0",
    "@cognite/data-exploration": "5.54.4",
    "@cognite/data-exploration-v4": "npm:@cognite/data-exploration@4.38.0",
    "@cognite/react-document-search": "^0.2.0",
    "@cognite/react-errors": "^1.0.4",
    "@cognite/react-feature-flags": "^1.0.0",
    "@cognite/react-picture-annotation": "3.0.1",
    "@cognite/reveal": "^3.3.0",
    "@cognite/sdk": "^7.13.0",
    "@cognite/sdk-provider": "^2.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.2",
    "@recoiljs/refine": "^0.1.1",
    "@sentry/browser": "^7.19.0",
    "@types/react-datepicker": "^3.1.7",
    "@types/react-highlight-words": "^0.16.3",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "antd": "^4.21.0",
    "i18next": "^21.8.14",
