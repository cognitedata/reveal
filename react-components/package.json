{
  "name": "@cognite/reveal-react-components",
  "version": "0.80.1",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "imports": {
    "#test-utils/*": "./tests/tests-utilities/*.ts",
    "#test-components/*": "./tests/tests-utilities/*.tsx"
  },
  "types": "./dist/index.d.ts",
  "type": "module",
  "author": {
    "name": "Christopher Tannum"
  },
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "build:watch:yalc": "YALC=true vite build --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --test",
    "tsc": "tsc",
    "typecheck:source": "tsc --noEmit --project tsconfig.build.json",
    "typecheck:test": "tsc --noEmit --project tsconfig.test.json",
    "lint": "eslint --cache --max-warnings 0 --ext=.tsx,.ts .",
    "test": "vitest run --dom --silent",
    "test:verbose": "vitest run --dom",
    "test:coverage": "vitest run --dom --coverage --silent",
    "test:playwright": "playwright test",
    "test:watch": "vitest watch --dom",
    "check-circular-deps": "madge --circular --extensions ts,tsx --exclude 'setClippingPlanes.ts$|CommandButtons.tsx$' ./src",
    "pull-keys": "cdf-i18n-utils-cli pull-keys-from-remote --namespace reveal-react-components --path ./src/common/i18n && yarn sort-keys",
    "remove-deleted": "cdf-i18n-utils-cli remove-deleted-keys-from-remote --namespace reveal-react-components --path ./src/common/i18n",
    "save-missing": "cdf-i18n-utils-cli save-missing-keys-to-remote --namespace reveal-react-components --path ./src/common/i18n",
    "sort-keys": "cdf-i18n-utils-cli sort-local-keys --namespace reveal-react-components --path ./src/common/i18n"
  },
  "peerDependencies": {
    "@cognite/reveal": "4.25.1",
    "@cognite/sdk": "^10.0.0",
    "react": ">=18",
    "react-dom": ">=18",
    "styled-components": "^6.1.12"
  },
  "dependencies": {
    "@cognite/cogs-lab": "9.0.0-alpha.160",
    "@cognite/cogs.js": "^10.36.0",
    "@cognite/signals": "^0.0.4",
    "@floating-ui/dom": "^1.0.0",
    "@tanstack/react-query": "^5.32.0",
    "assert": "^2.1.0",
    "lodash": "^4.17.21",
    "react-draggable": "^4.4.6",
    "react-resizable": "^3.0.5"
  },
  "devDependencies": {
    "@cognite/cdf-i18n-utils": "^0.7.5",
    "@cognite/cdf-utilities": "^3.6.0",
    "@cognite/reveal": "4.25.1",
    "@cognite/sdk": "^10.0.0",
    "@playwright/test": "1.49.0",
    "@storybook/addon-essentials": "8.4.5",
    "@storybook/addon-interactions": "8.4.5",
    "@storybook/addon-links": "8.4.5",
    "@storybook/blocks": "8.4.5",
    "@storybook/react": "8.4.5",
    "@storybook/react-vite": "8.4.5",
    "@storybook/test": "8.4.5",
    "@tanstack/react-query-devtools": "^5.32.0",
    "@testing-library/react": "^16.0.0",
    "@tippyjs/react": "4.2.6",
    "@types/lodash": "^4.17.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/react-resizable": "^3.0.7",
    "@types/three": "^0.166.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.0.6",
    "core-js": "^3.6.5",
    "eslint": "^8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.4.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^7.0.0",
    "eslint-plugin-react": "^7.34.1",
    "happy-dom": "^17.0.0",
    "locize-cli": "^8.0.1",
    "madge": "^8.0.0",
    "moq.ts": "10.0.8",
    "playwright": "1.49.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0",
    "storybook": "^8.3.5",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.12",
    "three": "0.166.1",
    "ts-loader": "^9.5.1",
    "typescript": "5.8.3",
    "vite": "^6.2.7",
    "vite-plugin-dts": "^4.0.0",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vitest": "^3.0.6"
  },
  "packageManager": "yarn@4.5.2",
  "files": [
    "dist"
  ]
}
