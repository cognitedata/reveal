{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "../../dist/out-tsc",
        "types": [
            "node"
        ]
    },
    "files": [
        "../../node_modules/@nx/react/typings/cssmodule.d.ts",
        "../../node_modules/@nx/react/typings/image.d.ts"
    ],
    "exclude": [
        "**/*.spec.ts",
        "**/*.spec.tsx",
        "**/*.test.tsx",
        "**/*.test.tsx",
        "**/*.stories.ts",
        "**/*.stories.js",
        "**/*.stories.jsx",
        "**/*.stories.tsx",
        "**/mocks/**.*",
        "jest.config.ts",
        "node_modules"
    ],
    "include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx"
    ]
}
