{
  "sourceRoot": "libs/platypus-common-utils/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/platypus-common-utils/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/platypus-common-utils"],
      "options": {
        "jestConfig": "libs/platypus-common-utils/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
