{
  "name": "@cognite/react-demo-app",
  "version": "0.3.2",
  "private": true,
  "scripts": {
    "start": "./scripts/start.sh",
    "build": "./scripts/build.sh",
    "test": "./scripts/test.sh",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}' 'public/index.html'",
    "eslint:changed": "eslint --max-warnings 0 --color --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "testcafe:run": "BASE_URL=http://localhost:11111 testcafe 'chromium:headless --no-sandbox' 'testcafe/desktop' --stop-on-first-fail",
    "testcafe:run-live": "BASE_URL=http://localhost:11111 testcafe 'chrome' 'testcafe/desktop' --live",
    "testcafe:start": "./scripts/testcafe-serve-run.sh",
    "testcafe:start-live": "./scripts/testcafe-start.sh",
    "testcafe:build": "./scripts/testcafe-build.sh",
    "testcafe:serve": "serve -s build -l 11111"
  },
  "dependencies": {
    "@cognite/cogs.js": "2.8.0",
    "@cognite/intercom-helper": "^0.1.0",
    "@cognite/metrics": "^1.0.0",
    "@cognite/react-container": "^0.9.0",
    "@cognite/react-errors": "^0.0.4",
    "@cognite/react-i18n": "^0.3.0",
    "@cognite/sdk": "^3.4.0",
    "@sentry/browser": "^6.2.5",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-dom": "17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.8.10",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux": "^4.0.5",
    "styled-components": "^5.2.1"
  },
  "resolutions": {
    "webpack": "4.44.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@cognite/eslint-config": "^2.0.1",
    "@cognite/eslint-plugin": "^2.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.6",
    "@testing-library/testcafe": "^4.3.1",
    "@types/jest": "^26.0.21",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.6",
    "@types/sinon": "^9.0.11",
    "@types/styled-components": "^5.1.9",
    "babel-loader": "8.1.0",
    "concurrently": "^6.0.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^6.0.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-junit": "^12.0.0",
    "kill-port": "^1.6.1",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "serve": "^11.3.2",
    "sinon": "^9.2.4",
    "testcafe": "^1.14.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "node_modules",
      "__tests__",
      ".stories."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
