{
  "name": "cdf-sdk-singleton",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/cdf-sdk-singleton/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "npm": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": ["./scripts/npm.sh"]
      },
      "outputs": ["{workspaceRoot}/dist/libs/shared/cdf-sdk-singleton"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/cdf-sdk-singleton/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/cdf-sdk-singleton",
        "tsConfig": "libs/shared/cdf-sdk-singleton/tsconfig.lib.json",
        "project": "libs/shared/cdf-sdk-singleton/package.json",
        "entryFile": "libs/shared/cdf-sdk-singleton/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "glob": "libs/shared/cdf-sdk-singleton/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "babelUpwardRootMode": true
      }
    },
    "build-single-spa": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/cdf-sdk-singleton-single-spa",
        "tsConfig": "libs/shared/cdf-sdk-singleton/tsconfig.lib.json",
        "project": "libs/shared/cdf-sdk-singleton/package.json",
        "main": "libs/shared/cdf-sdk-singleton/src/index.ts",
        "generatePackageJson": false,
        "extractLicenses": true,
        "sourceMap": false,
        "vendorChunk": false,
        "optimization": true,
        "commonChunk": false,
        "runtimeChunk": false,
        "compiler": "tsc",
        "assets": [],
        "fileReplacements": [
          {
            "replace": "libs/shared/cdf-sdk-singleton/src/set-public-path.ts",
            "with": "libs/shared/cdf-sdk-singleton/src/environments/single-spa/set-public-path.ts"
          }
        ],
        "webpackConfig": "libs/shared/cdf-sdk-singleton/webpack-config.js"
      }
    },
    "serve-static": {
      "executor": "@nrwl/web:file-server",
      "defaultConfiguration": "development",
      "options": {
        "staticFilePath": "dist/libs/shared/cdf-sdk-singleton",
        "buildTarget": "cdf-sdk-singleton:build-single-spa",
        "port": 3005
      },
      "configurations": {
        "production": {
          "buildTarget": "cdf-sdk-singleton:build-single-spa:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/cdf-sdk-singleton/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
