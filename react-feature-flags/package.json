{
  "name": "@cognite/react-feature-flags",
  "version": "0.0.2",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "dist",
    "jest-mocks.js"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "test": "jest",
    "esCheck": "es-check es5 './dist/index.js'",
    "build": "yarn clean && rollup -c && yarn esCheck",
    "build:watch": "yarn clean && rollup -c --watch && yarn esCheck",
    "watch": "yarn build --watch",
    "lint": "yarn concurrently \"yarn prettier\" \"yarn eslint 'src/**/*.{ts,tsx}'\"",
    "lint:fix": "yarn prettier --write && yarn eslint 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier -l 'src/**/*'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cognitedata/frontend"
  },
  "homepage": "https://github.com/cognitedata/frontend/tree/master/packages/browser/react/react-feature-flags",
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@cognite/eslint-plugin": "^1.3.0",
    "@cognite/testing": "^0.0.15",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.4.1",
    "rollup": "^1.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-typescript2": "^0.19.2"
  },
  "dependencies": {
    "unleash-proxy-client": "^1.0.1"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  }
