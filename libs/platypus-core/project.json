{
  "sourceRoot": "libs/platypus-core/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/platypus-core/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/platypus-core"],
      "options": {
        "jestConfig": "libs/platypus-core/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
