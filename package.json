    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@cognite/eslint-config": "^1.6.0",
    "@cognite/eslint-plugin": "^1.3.0",
    "@storybook/addon-docs": "^6.0.22",
