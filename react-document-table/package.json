    "lint:fix": "yarn prettier --write && yarn eslint 'src/**/*.{ts,tsx}' --fix",
    "prettier": "prettier -l 'src/**/*'",
    "prepublishOnly": "yarn build",
    "test": "yarn jest",
    "test:watch": "yarn jest --watchAll",
    "storybook": "start-storybook -p 6006"
