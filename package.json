{
  "name": "@cognite/cdf-raw-explorer",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "start": "env GENERATE_SOURCEMAP=false BROWSER=none WDS_SOCKET_PORT=3018 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3018 react-app-rewired start",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test --passWithNoTests",
    "lint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "pull-keys": "cdf-i18n-utils-cli pull-keys-from-remote --namespace raw-explorer",
    "remove-deleted": "cdf-i18n-utils-cli remove-deleted-keys-from-remote --namespace raw-explorer",
    "save-missing": "cdf-i18n-utils-cli save-missing-keys-to-remote --namespace raw-explorer"
  },
  "dependencies": {
