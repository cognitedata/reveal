{
  "name": "@cognite/cdf-access-management",
  "description": "Access management subapp of fusion.cognite.com",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "env BROWSER=none WDS_SOCKET_PORT=8000 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=8000 rescripts start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -p @cognite/cdf-access-management --outputFilename index.js --destinationPath ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "jest src",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .jsx,.js,.tsx,.ts --fix",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)"
  },
  "dependencies": {
    "@cognite/acl-protos": "^0.14.6",
    "@cognite/cdf-utilities": "^0.10.1",
    "@cognite/cogs.js": "^3.8.1",
    "@cognite/login-utils": "^1.2.3",
    "@cognite/sdk": "^6.3.2",
    "@cognite/sdk-provider": "^1.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.2",
    "@rescripts/cli": "^0.0.16",
    "@rescripts/utilities": "^0.0.8",
    "@sentry/browser": "^6.2.5",
    "@types/lodash": "^4.14.168",
    "antd": "^4.15.1",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "query-string": "^7.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-query": "^3.13.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "single-spa-react": "^4.1.1",
    "styled-components": "^5.2.3",
    "systemjs-webpack-interop": "^2.3.6"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.0.0",
    "@cognite/eslint-config": "^2.0.1",
    "@cognite/eslint-plugin": "^2.0.0",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rescripts/rescript-use-babel-config": "^0.0.12",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.39",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/single-spa-react": "^3.0.1",
    "@types/sinon": "^10.0.0",
    "@types/styled-components": "^5.1.9",
    "concurrently": "^6.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^4.0.1",
    "jest": "26",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "postcss-prefixwrap": "^1.22.1",
    "prettier": "^2.2.1",
    "sinon": "^10.0.0",
    "style-loader": "^2.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
