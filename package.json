{
  "name": "@cognite/cdf-data-catalog",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "start": "env GENERATE_SOURCEMAP=false BROWSER=none WDS_SOCKET_PORT=3080 WDS_SOCKET_HOST=localhost PORT=3080 react-app-rewired --max_old_space_size=2000 start",
    "build": "react-app-rewired --max_old_space_size=4096 build",
    "test": "react-app-rewired test",
    "lint": "yarn prettier && yarn eslint",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "prettier:fix": "prettier --write .prettierrc -l \"src/**/*.{js,ts,tsx}\"",
    "pull-keys": "cdf-i18n-utils-cli pull-keys-from-remote --namespace data-catalog",
    "remove-deleted": "cdf-i18n-utils-cli remove-deleted-keys-from-remote --namespace data-catalog",
    "save-missing": "cdf-i18n-utils-cli save-missing-keys-to-remote --namespace data-catalog"
  },
  "dependencies": {
    "@cognite/cdf-i18n-utils": "0.6.1",
    "@cognite/cdf-utilities": "^3.2.0",
    "@cognite/cogs.js": "9.8.0",
    "@cognite/metrics": "^1.2.1",
    "@cognite/react-feature-flags": "^1.0.0",
    "@cognite/sdk": "^6.3.4",
    "@cognite/sdk-provider": "^2.0.0",
    "@cognite/sdk-react-query-hooks": "^6.0.3",
    "@types/lodash": "^4.14.175",
    "antd": "^4.16.13",
    "copy-to-clipboard": "^3.3.2",
    "copy-webpack-plugin": "^11.0.0",
    "cronstrue": "^1.119.0",
    "eslint-plugin-promise": "^5.1.1",
    "formik": "^2.2.9",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-iframe": "^1.8.0",
    "react-query": "^3.34.16",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.0",
    "single-spa-react": "^3.2.0",
    "styled-components": "^5.3.1",
    "systemjs-webpack-interop": "^2.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/single-spa-react": "^2.12.0",
    "@types/styled-components": "^5.1.15",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less-loader": "^6.2.0",
    "postcss-prefixwrap": "^1.29.0",
    "prettier": "^2.4.1",
    "react-app-rewired": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "style-loader": "^1.3.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "nth-check": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14"
  }
}
