{
  "name": "cdf-sdk-singleton",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/cdf-sdk-singleton/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "npm": {
      "executor": "nx:run-commands",
      "dependsOn": ["build"],
      "options": {
        "commands": ["./scripts/npm.sh"]
      },
      "outputs": ["{workspaceRoot}/dist/libs/shared/cdf-sdk-singleton"]
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/shared/cdf-sdk-singleton/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/cdf-sdk-singleton",
        "tsConfig": "libs/shared/cdf-sdk-singleton/tsconfig.lib.json",
        "project": "libs/shared/cdf-sdk-singleton/package.json",
        "entryFile": "libs/shared/cdf-sdk-singleton/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "updateBuildableProjectDepsInPackageJson": true,
        "buildableProjectDepsInPackageJsonType": "dependencies",
        "assets": [
          {
            "glob": "libs/shared/cdf-sdk-singleton/README.md",
            "input": ".",
            "output": "."
          }
        ],
        "babelUpwardRootMode": true
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/cdf-sdk-singleton/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
