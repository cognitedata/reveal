  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "^1.10.7",
    "@cognite/cdf-ui-cli": "^1.0.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@commitlint/travis-cli": "^12.1.4",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/single-spa-react": "^2.12.0",
    "@types/sinon": "^9.0.10",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "babel-eslint": "10.1.0",
    "commitizen": "^4.2.4",
    "concurrently": "^5.3.0",
    "cross-var": "^1.1.0",
    "customize-cra": "^1.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-testing-library": "^5.8.0",
    "husky": "^4.3.7",
    "less": "3.12.2",
    "less-loader": "7.0.1",
    "path-browserify": "^1.0.1",
    "postcss-prefixwrap": "^1.20.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "react-app-rewired": "^2.2.1",
    "semantic-release": "^20.1.0",
    "sinon": "^9.2.3",
    "style-loader": "^1.3.0",
    "typescript": "4.0.3"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-dnd-html5-backend)/"
    ],
    "moduleNameMapper": {
      "^d3-(.*)$": "d3-$1/dist/d3-$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/src/pages",
      "<rootDir>/src/components"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog",
      "maxLineWidth": 50
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "resolutions": {
    "styled-components": "^5.3.6",
    "three": "0.136.0",
    "@types/three": "0.136.0",
    "@cognite/data-exploration/react-dnd": "^15.1.2",
    "@cognite/data-exploration/react-dnd-html5-backend": "^15.1.2"
  }
}
