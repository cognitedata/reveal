{
  "name": "@cognite/reveal-examples",
  "description": "Reveal examples",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env HTTPS=true react-app-rewired start",
    "start:nossl": "cross-env HTTPS=false react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --testPathIgnorePatterns 'src/visual_tests'",
    "test:visual": "jest --runInBand --detectOpenHandles --verbose false -c src/visual_tests/jest.config.js",
    "snapshots:update": "start-server-and-test 'yarn start:nossl' http://localhost:3000 'yarn snapshots:capture'",
    "snapshots:capture": "jest --runInBand --detectOpenHandles --updateSnapshot -c src/visual_tests/jest.config.js",
    "ci:nossl": "start-server-and-test 'yarn start:nossl' http://localhost:3000 'yarn test:visual'",
    "eject": "react-scripts eject"
  },
  "author": "Lars Moastuen <lars.moastuen@cognite.com>",
  "contributors": [
    "Lars Moastuen <lars.moastuen@cognite.com>",
    "Joachim Bj√∏rne <joachim.bjorne@cognite.com>",
    "Christopher Tannum <christopher.tannum@cognite.com>",
    "Maksim Nesterenko <maksim.nesterenko@cognite.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@cognite/potree-core": "^1.1.3",
    "@cognite/reveal": "link:../viewer/dist",
    "@cognite/sdk": "^2.31.0",
    "camera-controls": "^1.21.0",
    "dat.gui": "^0.7.7",
    "gl-matrix": "^3.3.0",
    "hold-event": "^0.0.1",
    "install": "^0.13.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.1.1",
    "three": "^0.115.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/dat.gui": "^0.7.5",
    "@types/jest": "^24.0.0",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/node": "^12.0.0",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "cross-env": "^7.0.2",
    "jest-environment-puppeteer": "^4.4.0",
    "jest-image-snapshot": "^4.0.2",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^2.0.5",
    "puppeteer": "^4.0.0",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "3.4.1",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^26.1.1",
    "typescript": "~3.7.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
