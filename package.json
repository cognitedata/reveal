{
  "name": "@cognite/cdf-document-search-ui",
  "version": "0.2.0",
  "private": true,
  "contributors": [
    {
      "name": "Unstructured search (grep)",
      "url": "https://github.com/orgs/cognitedata/teams/grep"
    }
  ],
  "scripts": {
    "start": "BROWSER=none WDS_SOCKET_PORT=3016 WDS_SOCKET_HOST=localhost SKIP_PREFLIGHT_CHECK=true PORT=3016 rescripts start",
    "build": "rescripts --max_old_space_size=4096 build",
    "generate-preview-artifacts": "cdf-ui-cli -pr -b cdf-hub-production -p @cognite/cdf-document-search-ui -o index.js -d ./build",
    "build:preview": "yarn build && yarn generate-preview-artifacts",
    "test": "rescripts test",
    "lint": "concurrently \"yarn prettier\" \"yarn eslint\"",
    "lint:fix": "prettier --config .prettierrc --write 'src/**/*.{js,ts,tsx}' && yarn eslint --fix",
    "eslint": "yarn eslint:changed 'src/**/*.{ts,tsx}'",
    "eslint:changed": "eslint --config .eslintrc.production.js",
    "prettier": "prettier --config .prettierrc -l 'src/**/*.{js,ts,tsx}' || (echo \nThese files are not formatted correctly && false)",
    "storybook": "start-storybook -p 6006"
  },
  "dependencies": {
    "@cognite/cdf-utilities": "0.10.1",
    "@cognite/cogs.js": "5.2.1",
    "@cognite/metrics": "0.1.7",
    "@cognite/sdk": "6.3.4",
    "@cognite/sdk-playground": "5.0.3",
    "@cognite/sdk-provider": "1.0.0",
    "@cognite/sdk-react-query-hooks": "6.0.3",
    "@cognite/z-index": "0.2.1",
    "@rescripts/cli": "0.0.14",
    "@rescripts/utilities": "0.0.7",
    "@sentry/browser": "5.30.0",
    "@xstate/inspect": "0.5.2",
    "@xstate/react": "1.6.3",
    "antd": "3.26.20",
    "framer-motion": "4.1.17",
    "fs-extra": "9.1.0",
    "lodash": "4.17.21",
    "query-string": "6.14.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "3.34.16",
    "react-router": "6.2.2",
    "react-router-dom": "6.2.2",
    "react-scripts": "3.4.3",
    "single-spa-react": "3.2.0",
    "styled-components": "5.3.3",
    "systemjs-webpack-interop": "2.3.7",
    "xstate": "4.30.3"
  },
  "devDependencies": {
    "@cognite/babel-plugin-styled-components": "1.10.7",
